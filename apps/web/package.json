{
    "name": "@crates/web",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "predev": "bun --filter=@crates/shared run dev && bun run build:dsp",
        "dev": "vite",
        "build:dsp": "cd ../../libs/dsp && cargo build --release --target wasm32-unknown-unknown",
        "build": "tsc -b && vite build",
        "preview": "vite preview",
        "format": "bunx biome format src --write .",
        "lint": "bunx biome lint --error-on-warnings src",
        "lint:ts": "tsc --noEmit"
    },
    "dependencies": {
        "@crates/shared": "workspace:*",
        "solid-js": "^1.9.7"
    },
    "devDependencies": {
        "typescript": "~5.8.3",
        "vite": "^7.0.4",
        "vite-plugin-solid": "^2.11.7"
    }
}
